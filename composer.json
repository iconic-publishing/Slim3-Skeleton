{
    "name": "iconic-publishing/slim3-skeleton",
    "description": "A skeleton starter structure for Slim 3",
    "keywords": ["framework", "slim", "iconic publishing"],
    "license": "MIT",
    "type": "project",
	"require": {
		"vlucas/phpdotenv": "^2.4",
        "symfony/var-dumper": "^4.0",
        "hassankhan/config": "^1.0",
		"robmorgan/phinx": "^0.9.2",
		"illuminate/translation": "^5.6",
        "illuminate/pagination": "^5.6",
        "illuminate/database": "^5.6",
        "nesbot/carbon": "^1.25",
        "doctrine/dbal": "^2.6",
        "fzaninotto/faker": "^1.7",
        "illuminate/console": "^5.6",
        "illuminate/events": "^5.6",
        "illuminate/filesystem": "^5.6",
        "symfony/event-dispatcher": "^4.0",
        "symfony/lock": "^4.0",
        "symfony/process": "^4.0",
        "psr/log": "^1.0",
        "dragonmantank/cron-expression": "^2.0",
        "symfony/dependency-injection": "^4.0",
        "symfony/http-kernel": "^4.0",
        "slim/slim": "^3.9",
        "slim/twig-view": "^2.3",
        "slim/flash": "^0.4.0",
        "slim/csrf": "^0.8.2",
        "respect/validation": "^1.1",
        "php-http/guzzle6-adapter": "^1.1",
        "mailgun/mailgun-php": "^2.4",
        "twilio/sdk": "^5.16",
		"google/recaptcha": "^1.1",
		"laravel/cashier": "^7.1",
        "moneyphp/money": "^3.1",
        "florianv/swap": "^3.3",
        "florianv/exchanger": "^1.0"
    },
    "autoload": {
        "psr-4": {
            "Base\\": "app"
        }
    },
    "scripts": {
        "post-root-package-install": [
            "php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ]
    }
}
